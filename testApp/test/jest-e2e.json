{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "../../",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "globals": {
        "ts-jest": {
            "diagnostics": false,
            "tsConfig": "<rootDir>/testApp/tsconfig.json"
        }
    },
    "collectCoverageFrom": ["<rootDir>/src/**/*.ts"]
}
