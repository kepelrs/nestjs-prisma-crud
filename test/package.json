{
    "name": "test",
    "version": "0.0.4",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "yarn run test:e2e",
        "test:watch": "yarn run test:e2e -- --watch",
        "test:cov": "yarn run test:e2e -- --coverage",
        "test:clear": "jest --clearCache",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "prisma migrate reset --force && jest --config ./test/jest-e2e.json --runInBand",
        "reset": "prisma migrate reset",
        "generate-migration": "prisma migrate dev --create-only",
        "migrate": "prisma migrate deploy",
        "scaffold-module": "nest g -c nestjs-prisma-crud-schematics crud-resource"
    },
    "dependencies": {
        "@nestjs/common": "10.4.19",
        "@nestjs/core": "10.4.19",
        "@nestjs/mapped-types": "2.1.0",
        "@nestjs/platform-express": "10.4.19",
        "@prisma/client": "6.12.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.2",
        "dotenv": "^17.0.0",
        "object-traversal": "1.0.1",
        "rimraf": "6.0.1",
        "rxjs": "7.8.2"
    },
    "devDependencies": {
        "@nestjs/cli": "10.4.9",
        "@nestjs/schematics": "10.2.3",
        "@nestjs/testing": "10.4.19",
        "@types/express": "5.0.3",
        "@types/jest": "30.0.0",
        "@types/node": "22.17.0",
        "@types/supertest": "6.0.3",
        "@typescript-eslint/eslint-plugin": "8.38.0",
        "@typescript-eslint/parser": "8.38.0",
        "eslint": "9.31.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.3",
        "jest": "30.0.5",
        "prettier": "3.5.3",
        "prisma": "6.12.0",
        "supertest": "7.1.4",
        "ts-jest": "29.4.1",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.8.3"
    },
    "resolutions": {
        "**/@typescript-eslint/eslint-plugin": "8.38.0",
        "**/@typescript-eslint/parser": "8.38.0",
        "**/jest": "30.0.5",
        "**/ts-jest": "29.4.1",
        "**/typescript": "5.8.3"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
